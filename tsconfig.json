{
    "include": [
        "src/**/*"
    ],
    "compilerOptions": {
        "strict": true,
        "outDir": "./dist/",
        "sourceMap": true,
        "target": "es5",
        "jsx": "react",
        "lib":["dom", "es6", "es2016.array.include", "es2017.object"],
        "allowJs": true,
        "experimentalDecorators": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "allowSyntheticDefaultImports": true
    }
}